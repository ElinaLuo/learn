{
  "name": "@seed/ui-base",
  "version": "1.3.1",
  "description": "a component for seed design",
  "keywords": [
    "seed",
    "design"
  ],
  "author": "jianhua.zhong <jianhua.zhong@dianping.com>",
  "homepage": "https://seed.sankuai.com/#/",
  "license": "ISC",
  "module": "es/utils.js",
  "main": "lib/utils.js",
  "directories": {
    "lib": "lib",
    "es": "es",
    "test": "__tests__"
  },
  "sideEffects": [
    "*.css"
  ],
  "publishConfig": {
    "registry": "http://r.npm.sankuai.com/"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.dianpingoa.com/f2e/seed-ui-components.git"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm-run-all clean compile:js",
    "clean": "rm -rf ./es ./lib",
    "compile:js": "run-p compile:es6 compile:commonjs",
    "compile:es6": "BABEL_ENV=es NODE_ENV=production ../../node_modules/.bin/babel --root-mode upward ./src --out-dir ./es",
    "compile:commonjs": "NODE_ENV=production ../../node_modules/.bin/babel --root-mode upward ./src --out-dir ./lib"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.5.5"
  }
}