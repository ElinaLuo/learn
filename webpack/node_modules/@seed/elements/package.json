{
  "name": "@seed/elements",
  "version": "1.5.1",
  "description": "elements component for seed design",
  "keywords": [
    "seed-design",
    "react",
    "vue"
  ],
  "author": "jianhua.zhong <jianhua.zhong@dianping.com>",
  "homepage": "https://seed.sankuai.com/#/",
  "license": "MIT",
  "module": "es/react.js",
  "main": "lib/react.js",
  "directories": {
    "lib": "lib",
    "es": "es",
    "test": "__tests__"
  },
  "sideEffects": [
    "*.css"
  ],
  "publishConfig": {
    "registry": "http://r.npm.sankuai.com/"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@git.dianpingoa.com/f2e/seed-ui-components.git"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm-run-all clean compile:js",
    "clean": "rm -rf ./es ./lib",
    "compile:js": "run-p compile:es6 compile:commonjs compile:vue",
    "compile:vue": "NODE_ENV=production rollup -c ../../rollup.vue.config.js",
    "compile:es6": "BABEL_ENV=es NODE_ENV=production ../../node_modules/.bin/babel --root-mode upward ./src --out-dir ./es --copy-files",
    "compile:commonjs": "NODE_ENV=production ../../node_modules/.bin/babel --root-mode upward ./src --out-dir ./lib --copy-files"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.5.5",
    "@dp/rlu2": "^1.0.10",
    "@dp/util-color": "^0.0.14",
    "@seed/ui-base": "^1.3.1",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  }
}