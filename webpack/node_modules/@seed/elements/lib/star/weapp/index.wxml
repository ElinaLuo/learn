<view class="seed-star-wrap {{classname}}">
  <view class="seed-star-icon">
    <view data-index="10" class="seed-star seed-star-{{star}}" style="width: {{starStyle.width}}; height: {{starStyle.height}}; margin-right: {{starStyle.marginRight}}" bindtap="onTapStar"></view>
    <view data-index="20" class="seed-star seed-star-{{star}}" style="width: {{starStyle.width}}; height: {{starStyle.height}}; margin-right: {{starStyle.marginRight}}" bindtap="onTapStar"></view>
    <view data-index="30" class="seed-star seed-star-{{star}}" style="width: {{starStyle.width}}; height: {{starStyle.height}}; margin-right: {{starStyle.marginRight}}" bindtap="onTapStar"></view>
    <view data-index="40" class="seed-star seed-star-{{star}}" style="width: {{starStyle.width}}; height: {{starStyle.height}}; margin-right: {{starStyle.marginRight}}" bindtap="onTapStar"></view>
    <view data-index="50" class="seed-star seed-star-{{star}}" style="width: {{starStyle.width}}; height: {{starStyle.height}}; margin-right: {{starStyle.marginRight}}" bindtap="onTapStar"></view>
  </view>
    <view wx:if="{{score}}" class="seed-star-score seed-score-{{star}}" style="font-size: {{textStyle.fontSize}}; margin-left: {{textStyle.marginLeft}}">{{score}}</view>
</view>
